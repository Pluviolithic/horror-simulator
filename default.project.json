{
  "name": "horror-simulator",
  "base": "default.base.rbxl",
  "build": "_Export/HorrorSimulator.rbxlx",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Common": {
        "$path": "src/shared",
        "$clearOnSync": true,
        "lib": {
          "$className": "Folder",
          "$path": "./Packages"
        }
      }
    },

    "ServerScriptService": {
      "Server": {
        "$clearOnSync": true,
        "$path": "src/server",
        "lib": {
          "$className": "Folder",
          "$path": "./ServerPackages"
        }
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$clearOnSync": true,
          "$path": "src/client"
        }
      }
    }

  }
}
